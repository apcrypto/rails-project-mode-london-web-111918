<center>
  <%= form_for @owner do |f| %>

  <p><%= f.label 'First Name' %></p>
  <p><%= f.text_field :first_name %></p>

  <p><%= f.label 'Last Name' %></p>
  <p><%= f.text_field :last_name %></p>

  <p><%= f.label 'Username' %></p>
  <p><%= f.text_field :username %></p>

  <p><%= f.label 'Address' %></p>
  <p><%= f.text_field :address %></p>

  <p><%= f.label 'Telephone Number' %></p>
  <p><%= f.text_field :telephone_number %></p>

  <p><%= f.label 'Email' %></p>
  <p><%= f.text_field :email %></p>

  <p><%= f.label 'Image URL' %></p>
  <p><%= f.text_field :img_url %></p>

  <p><%= f.label 'Password' %></p>
  <p><%= f.password_field :password %></p>

  <p><%= f.label 'Confirm Password' %></p>
  <p><%= f.password_field :password_confirmation %></p>

  <p><%= f.submit %></p>

<% end %>
</center>
